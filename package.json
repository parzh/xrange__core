{
  "name": "@xrange/core",
  "description": "The very core of `xrange`, provided as a separate package",
  "version": "0.0.1",
  "license": "MIT",
  "keywords": [
    "generator",
    "iterate",
    "iterator",
    "python",
    "range",
    "xrange"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node --require ts-node/register src",
    "clean": "rimraf dist/*",
    "clean:spare": "rimraf dist/**/*.impl.d.ts dist/typings/*.js",
    "prebuild": "npm run clean",
    "build": "tsc --project ./tsconfig.prod.json",
    "postbuild": "npm run clean:spare",
    "dev": "tsc --project ./tsconfig.dev.json"
  },
  "devDependencies": {
    "@types/node": "13.9.0",
    "rimraf": "3.0.2",
    "ts-node": "8.6.2",
    "typescript": "3.8.3"
  }
}
